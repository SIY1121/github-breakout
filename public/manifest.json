{
  "name": "GitHub Breakout",
  "version": "1.1.0",
  "manifest_version": 2,
  "description": "Breakout!",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["main.js"]
    }
  ],
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "64": "icons/64.png",
    "128": "icons/128.png",
    "512": "icons/512.png"
  }
}